
#Mongo Config
spring.data.mongodb.host=localhost
spring.data.mongodb.port=27017
spring.data.mongodb.database=geo-mongo
spring.data.mongodb.repositories.enabled=true
spring.data.mongodb.username=admin
spring.data.mongodb.password=admin
spring.data.mongobee.changelogs=com.geodiff.changelogs

# RabbitMQ Config
spring.data.rabbitmq.amqp_uri=amqp://guest:guest@localhost:5672/%2f
spring.data.rabbitmq.res_queue=RESULT_QUEUE
spring.data.rabbitmq.task_queue=TASK_QUEUE
spring.data.rabbitmq.res_xchg=RESULT_XCHG

